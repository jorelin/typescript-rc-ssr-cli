{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "plugins": [
        "react-hooks"
    ],
    "rules": {
        "eofline": false,
        "max-line-length": [true, 160],
        "member-access": false,
        "array-type": [false],
        "ordered-imports": [false],
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "interface-name": [
            false
        ],
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn",
        "no-console": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": []
}