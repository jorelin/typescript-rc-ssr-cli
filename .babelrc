{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "ie >= 10",
            "Chrome >= 40"
          ]
        }
      },
      // { "modules": false }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "dynamic-import-webpack",
    "transform-object-rest-spread",
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}